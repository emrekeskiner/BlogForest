@model CreateCategoryDto
@{
    ViewData["Title"] = "Yeni Kategori";
    Layout = "~/Views/Layout/_WriterLayout.cshtml";
}

<div class="col-lg-12">
    <div class="card mb-4">

        <div class="table-responsive p-3">

            <form method="post">
                <label>Kategori Adı</label>
                <input type="text" asp-for="CategoryName" class="form-control-range" />
                <br />
                <button class="btn btn-primary">Kaydet</button>
            </form>
        </div>
    </div>
</div>

